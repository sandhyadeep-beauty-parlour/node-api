{
  "forgotPasswordSchema": {
    "properties": {
      "verification_code": {
        "type": "string",
        "example": "e35rpu"
      },
      "password": {
        "type": "string",
        "example": "tudip123"
      }
    }
  },
  "forgotPasswordSuccessSchema": {
    "properties": {
      "access_token": {
        "type": "string",
        "example": "eyJraWQ5zdG9ybXBhdGguY29tL3YxL2FwcGxpY2F0aW9ucy82jyd2wV1R3aG9uWko2M3B1YUFzWGtmMVo2I89i0w0ic3ViIs"
      },
      "token_type": {
        "type": "string",
        "example": "Bearer"
      },
      "expires_in": {
        "type": "integer",
        "example": 3600
      },
      "user": {
        "$ref": "./completeUserResponseModel.json#/CompleteUserSchema"
      }
    }
  }
}